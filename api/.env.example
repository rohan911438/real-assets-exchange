// Environment variables template
NODE_ENV=development
PORT=5000

# Database
DATABASE_URL="postgresql://username:password@localhost:5432/rwa_dex"

# Redis
REDIS_URL="redis://localhost:6379"

# JWT
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=7d

# Blockchain
MANTLE_RPC_URL=https://rpc.mantle.xyz
MANTLE_TESTNET_RPC_URL=https://rpc.testnet.mantle.xyz
PRIVATE_KEY=your_private_key_here

# Contract Addresses (Mantle Testnet)
DEX_CORE_ADDRESS=0x...
RWA_FACTORY_ADDRESS=0x...
COMPLIANCE_REGISTRY_ADDRESS=0x...
LENDING_PROTOCOL_ADDRESS=0x...
YIELD_DISTRIBUTOR_ADDRESS=0x...
PRICE_ORACLE_ADDRESS=0x...

# API Keys
COINMARKETCAP_API_KEY=your_api_key
CHAINLINK_API_KEY=your_api_key

# AI Engine
AI_ENGINE_URL=http://localhost:8001
AI_ENGINE_API_KEY=your_ai_engine_key

# External Services
KYC_PROVIDER_API_KEY=your_kyc_provider_key
NOTIFICATION_SERVICE_KEY=your_notification_key

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX_REQUESTS=100

# Cache TTL (in seconds)
CACHE_TTL_PRICES=30
CACHE_TTL_PORTFOLIO=60
CACHE_TTL_ASSETS=3600