{
  "name": "@rwa-dex/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for RWA DEX platform with MetaMask integration for Mantle Sepolia",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm test",
    "demo": "cd ../demo && python -m http.server 8000"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "ethers": "^6.8.0",
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "jest": "^29.6.1",
    "@types/jest": "^29.5.3",
    "ts-jest": "^29.1.1",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "keywords": [
    "RWA", 
    "DeFi", 
    "SDK", 
    "Mantle", 
    "Sepolia", 
    "TypeScript", 
    "MetaMask", 
    "Web3", 
    "Ethereum", 
    "Real-World-Assets",
    "Decentralized-Exchange",
    "Blockchain"
  ],
  "author": "RWA DEX Team <dev@rwa-dex.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rwa-dex/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/rwa-dex/sdk/issues"
  },
  "homepage": "https://rwa-dex.com",
  "engines": {
    "node": ">=16.0.0"
  }
}